{
  "num_epochs": 1,
  "seed": 1234,
  "pretrained_model_name_or_path": "/home/dcuuser/dxm/diffusers/model/stable-diffusion-v1-5",
  "dataset_dir": "./pokemon-blip-captions",
  "output_dir": "FineTunedStableDiffusion",
  "ckpt_name": "pokemon",
  "save_interval": 50,
  "use_lora": {
    "action": false,
    "output_dir": "StableDiffusion-Lora"
  },
  "resolution": 512,
  "center_crop": false,
  "random_flip": false,
  "use_fp16": {
    "enabled": true,
    "auto_cast": false,
    "loss_scale": 0,
    "initial_scale_power": 16,
    "loss_scale_window": 1000,
    "hysteresis": 2,
    "consecutive_hysteresis": false,
