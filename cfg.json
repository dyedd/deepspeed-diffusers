  "log_interval": 10,
  "optimizer": {
    "type": "AdamW",
    "params": {
      "lr": 1e-5,
      "betas": [
        0.9,
        0.999
      ],
      "eps": 1e-08,
      "weight_decay": 0.01
    }
  },
  "scheduler": {
    "type": "WarmupLR",
    "params": {
      "warmup_min_lr": 0,
      "warmup_max_lr": 0.0001,
      "warmup_num_steps": 1000
    }
  },
  "wall_clock_breakdown": false,
  "wandb": {
    "enabled": true,
    "group": "ddp",
    "project": "deepspeed-diffusers"
  }
