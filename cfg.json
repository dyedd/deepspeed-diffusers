{
  "num_epochs": 1,
  "use_lora": {
    "action": false,
    "output_dir": "StableDiffusion-Lora"
  },
  "save_interval": 50,
  "ckpt_name": "pokemon",
  "pretrained_model_name_or_path": "/home/dcuuser/dxm/diffusers/model/stable-diffusion-v1-5",
  "output_dir": "FineTunedStableDiffusion",
  "dataset_dir": "./pokemon-blip-captions",
  "resolution": 512,
  "center_crop": false,
  "random_flip": false,
  "wandb_project": "deepspeed-diffusers",
  "use_fp16": true,
  "train_micro_batch_size_per_gpu": 2,
  "gradient_accumulation_steps": 1,
  "log_interval": 10,
  "communication_data_type": "fp16",
  "optimizer": {
    "type": "AdamW",
    "params": {
      "lr": 1e-5,
      "betas": [
        0.9,
        0.999
      ],
      "eps": 1e-08,
      "weight_decay": 0.01
    }
  },
  "scheduler": {
    "type": "WarmupLR",
    "params": {
      "warmup_min_lr": 0,
      "warmup_max_lr": 0.0001,
      "warmup_num_steps": 1000
    }
  },
  "wall_clock_breakdown": false
}
